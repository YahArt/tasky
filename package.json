{
  "name": "tasky",
  "version": "1.0.0",
  "description": "Yet another but refreshing todo app with gamification",
  "main": "main.js",
  "scripts": {
    "start": "concurrently --kill-others \"riot --type coffee -w app/components components\" \"browser-sync start --server --files '**/*.html, **/*.css, **/*.js' --no-notify\""
  },
