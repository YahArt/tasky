    "type": "git",
    "url": "git+https://github.com/YahArt/tasky.git"
  },
  "keywords": [
    "coffeescript",
    "riotjs"
  ],
  "author": "YahArt",
  "license": "MIT",
  "homepage": "https://github.com/YahArt/tasky#readme",
  "dependencies": {
    "mdbootstrap": "^4.7.7",
    "riot": "^3.13.2"
  },
  "devDependencies": {
    "coffeescript": "^2.4.1",
    "concurrently": "^4.1.0"
  }
}
